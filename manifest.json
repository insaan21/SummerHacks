{
    "name" : "test",
    "version" : "1.0",
    "manifest_version" : 2,
    "description": "Test plugin",
    "browser_action" : {
        "default_icon" : "images/rgbchromeblack.png", 
        "default_popup": "popup.html",
        "default_title": "Test"
    },
    "background": {
        "scripts" : ["backgrounds.js"]
    },
    "permissions" : [
        "background",
        "tabs",
        "http://*/*",
        "https://*/*",
        "storage"
    ]
}